  "contributors": [],
  "main": "./index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/jsBoot/gulp-jsdoc.git"
  },
  "scripts": {
    "test": "istanbul test _mocha --report html -- test/*.js --reporter spec",
