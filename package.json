  "devDependencies": {
    "mocha": "^3.2.0",
    "coveralls": "^2.11.15",
    "mocha-lcov-reporter": "~1.2.0",
    "istanbul": "^0.4.5",
    "event-stream": "~3.3.4",
    "should": "^11.1.1",
    "jshint": "^2.9.4",
    "gulp": "^3.9.1",
    "gulp-template": "~4.0.0",
