    "marked": "~0.3.1",
    "requizzle": "^0.2.0",
    "taffydb": "~2.7.2",
    "text-table": "~0.2.0",
    "through2": "^1.0.0",
    "vinyl-fs": "~0.3.0",
    "wrench": "~1.5.6"
  },
  "devDependencies": {
    "coveralls": "^2.7.1",
    "event-stream": "~3.1.0",
    "gulp": "^3.5.6",
    "gulp-eslint": "~0.1.2",
    "gulp-jshint": "^1.3.4",
    "gulp-template": "~0.1.1",
    "istanbul": "^0.2.4",
    "jshint": "^2.4.4",
    "jshint-stylish": "^0.1.5",
    "mocha": "^1.17.1",
    "mocha-lcov-reporter": "~0.0.1",
